{% extends '::base.html.twig' %}

{% block body -%}
    <h1>AbstractVideo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Internalname</th>
                <td>{{ entity.internalName }}</td>
            </tr>
            <tr>
                <th>Audiolanguage</th>
                <td>{{ entity.audioLanguage }}</td>
            </tr>
            <tr>
                <th>Subtitleslanguage</th>
                <td>{{ entity.subtitlesLanguage }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('abstractvideo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('abstractvideo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
